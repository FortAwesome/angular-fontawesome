{
  "name": "@fortawesome/angular-fontawesome",
  "version": "0.1.0",
  "description": "Angular Fontawesome, an Angular library",
  "private": true,
  "scripts": {
    "test": "ng test --watch=false --browsers=ChromeCI",
    "lint": "ng lint angular-fontawesome --type-check --format stylish",
    "precommit": "npm run lint && lint-staged",
    "start": "ng serve",
    "build:demo": "ng build --prod",
    "prebuild": "rimraf dist",
    "build": "ts-node ./tasks/build.ts"
  },
  "lint-staged": {
    "README.md": [
      "markdown-toc -i",
      "git add README.md"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FortAwesome/angular-fontawesome"
  },
  "contributors": [
    "Mike Wilkerson <mwilkerson@gmail.com>",
    "Yaroslav Admin <devoto13@gmail.com>",
    "Travis Chase <travis@fontawesome.com>",
    "Rob Madole <rob@fontawesome.com>",
    "Zeev Katz <zeevk6@gmail.com>",
    "Scott Cooper <scttcper@gmail.com>",
    "Brian Talbot <hi.talbs@gmail.com>",
    "Davide Pastore <pasdavide@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FortAwesome/angular-fontawesome/issues"
  },
  "homepage": "https://github.com/FortAwesome/angular-fontawesome",
  "dependencies": {
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "core-js": "^2.5.7",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.6",
    "@angular/language-service": "^6.0.6",
    "@types/fs-extra": "^5.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.3.4",
    "codelyzer": "~4.3.0",
    "fs-extra": "^6.0.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "^7.2.0",
    "ng-packagr": "4.0.0-rc.1",
    "protractor": "~5.3.2",
    "rimraf": "^2.6.2",
    "ts-node": "~6.1.2",
    "tslint": "~5.10.0",
    "typescript": "2.7.2"
  },
  "keywords": [
    "angular",
    "font",
    "awesome",
    "fontawesome",
    "icon",
    "svg"
  ]

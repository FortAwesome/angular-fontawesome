  "keywords": [
    "angular",
    "font",
    "awesome",
    "fontawesome",
    "icon",
    "svg"
  ],
  "peerDependencies": {
    "@angular/core": "^9.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/lib/public_api.ts",
      "flatModuleFile": "angular-fontawesome"
    },
    "dest": "./dist/angular-fontawesome"
  },
  "schematics": "./schematics/collection.json"
}
